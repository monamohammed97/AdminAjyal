{
     "rules": {
          "no-undef": "off",
          "react/prop-types": "off",
          "react/no-unescaped-entities": 0,
          "react/react-in-jsx-scope": "off"
     },
     "extends": [
          "plugin:react/recommended"
     ],
     "parserOptions": {
          "ecmaVersion": 2020,
          "sourceType": "module",
          "ecmaFeatures": {
               "jsx": true,
               "modules": true,
               "experimentalObjectRestSpread": true
          }
     }
}
